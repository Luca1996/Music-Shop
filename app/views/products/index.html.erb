<% @products.each do |prod| %>
    <p> <%= prod.title %></p> 
    <%= link_to  "Details", product_path(prod) %>
    <% if user_signed_in? %>
    <%= link_to  "Update", edit_product_path(prod) %>
    <%= link_to  "Delete", product_path(id: prod.id, authenticity_token: form_authenticity_token),:data => {:confirm => "You sure?",:method => "delete"} %>
    <% end %>
<% end %>